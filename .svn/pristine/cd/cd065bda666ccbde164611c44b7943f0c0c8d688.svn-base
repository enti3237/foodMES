    Public Function visible_Columns()
        For Each columns As DataGridViewColumn In DataGridView2.Columns
            If columns.HeaderCell.Style.BackColor = Color.Violet Then
                columns.Visible = True
            Else
                columns.Visible = False
            End If
        Next
    End Function

