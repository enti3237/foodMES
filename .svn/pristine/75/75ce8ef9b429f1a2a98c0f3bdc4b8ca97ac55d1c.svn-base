        DBT = Nothing

        StrSQL = "Select CR_Code, CR_Rev_No, CR_Sun, CR_PL_Code, CR_PL_Name, CR_Qty, CR_Date From CR_Contract_List with(NOLOCK) where CR_Code Like '%" & txtJobNo.Text & "%' And
