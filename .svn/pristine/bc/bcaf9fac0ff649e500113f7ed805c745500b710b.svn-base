                '#3-1 PO 목록 가져오기
                StrSQL = "SELECT CO.*, COL.* from CM_Order CO, CM_Order_List COL where CO.CO_Code = '" & tmpCOCode & "' And CO.CO_Rev_No = " & tmpCORevNo & " And CO.CO_Code = COL.CO_Code And CO.CO_Rev_No = COL.CO_Rev_No order by COL.CO_Sun asc "
                initTgtCount = DB_Select(DBTinit)
