        If Excel_Check = True Then
            xworkbook.Close()
            xls.Quit()
            xworksheet = Nothing
            xworkbook = Nothing
            xls = Nothing
            releaseObject(xworksheet)
            releaseObject(xworkbook)
            releaseObject(xls)
            Excel_Check = False

        End If

        xls = New Excel.Application
        xls.Visible = False
        xworkbook = xls.Workbooks.Open(Application.StartupPath + "\\Excel\\인수검사의뢰서.xlsx")
        xworksheet = xworkbook.Sheets(1)

        xls.DisplayAlerts = False
        xls.DisplayFormulaBar = False
        xls.DisplayStatusBar = False
        xls.ActiveWindow.DisplayWorkbookTabs = False
        xls.ActiveWindow.DisplayHeadings = False

        SetParent(xls.Hwnd, Panel8.Handle)
        SendMessage(xls.Hwnd, WM_SYSCOMMAND, SC_MAXIMIZE, 0)
        Excel_Check = True
    End Sub

