        Dim i%
        Dim DBT As DataTable, DBT2 As DataTable
        DBT = Nothing
        DBT2 = Nothing

        Try

            StrSQL = "SELECT * from CM_Stock_QC where CQ_Code = '" & txtCQCode2.Text & "' And CQ_Rev_No = (SELECT max(CQ_Rev_No) from CM_Stock_QC where CQ_Code = '" & txtCQCode2.Text & "' and CQ_Rev_No = '" & txtCQRev2.Text & "')"
