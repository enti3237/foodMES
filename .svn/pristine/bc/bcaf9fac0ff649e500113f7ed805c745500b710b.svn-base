        'Try

        '#1. popUp으로 PO 정보를 가져옴
        Dim pop1 As Frm_PO_POP = New Frm_PO_POP
            pop1.StartPosition = FormStartPosition.CenterScreen
            If (pop1.ShowDialog(Me) = DialogResult.OK) Then
                tmpCOCode = pop1.DataGridView1.Rows(pop1.DataGridView1.CurrentCell.RowIndex).Cells("CO_Code").Value.ToString
